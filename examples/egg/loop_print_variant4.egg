def myPrint(e): print(e) and

def loop(start, terminate, f):
  let i = [start] in
  if i[0] == terminate:
    []
  else:
    f(i[0]);
    i[0] := (i[0] + 1);
    loop(i[0], terminate, f)
in

def loop2(start, terminate, f):
  if start == terminate:
    []
  else:
    f(start);
    loop2(start + 1, terminate, f)
in

loop2(1, 5, myPrint)
